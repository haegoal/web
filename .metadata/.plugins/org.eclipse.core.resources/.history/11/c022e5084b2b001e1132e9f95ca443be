<script>
getlist();

	function getlist(){
			$.ajax({
				type:"get",
				url:"https://dapi.kakao.com/v2/local/search/keyword.json",
				dataType:"json",
				data:{query:"인천버거킹"},
				header:{"Authorization":"KakaoAK 7857aae77aaad7dfa832ea3c00257177"},
				success:function(data){
					console.log(data);
				}
			})
		}
</script>